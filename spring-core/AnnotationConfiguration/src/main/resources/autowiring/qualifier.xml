<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="mySqlConnector" class="com.examples.annotations.autowiring.qualifier.MySqlConnector">
	</bean>
	
	<bean id="oracleConnector" class="com.examples.annotations.autowiring.qualifier.OracleConnector">
	</bean>
	

	<bean id="mySqlConnection" class="com.examples.annotations.autowiring.qualifier.MySqlConnection">
	</bean>

	<bean id="oracleConnection" class="com.examples.annotations.autowiring.qualifier.OracleConnection">
	</bean>
</beans>